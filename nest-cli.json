{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "assets": ["**/*.json"],
  "watchAssets": true,
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "swc",
    "typeCheck": true,
    "assets": [
      "**/*.json"
    ],
    "watchAssets": true
  },
  "generatorOptions": {
    "spec": false
  }
}
